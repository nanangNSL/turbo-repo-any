{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": ["API_URL"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prod": {
      "cache": false,
      "dependsOn": ["^prod"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^test"]
    }
  }
}
